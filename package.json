{
    "name": "@fy-/ui",
    "version": "0.0.1",
    "author": "Florian 'Fy' Gasquez <m@fy.to>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fyvue/ui.git"
    },
    "bugs": {
        "url": "https://github.com/fyvue/ui/issues"
    },
    "homepage": "https://github.com/fyvue/ui#readme",
    "main": "dist/fyui.cjs.js",
    "module": "dist/fyui.es.js",
    "typings": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/fyui.es.js",
            "require": "./dist/fyui.cjs.js",
            "types": "./dist/types/index.d.ts"
        },
        "./style.css": {
            "import": "./dist/style.css",
            "require": "./dist/style.css"
        }
    },
    "peerDependencies": {
        "vue": "^3.2.x"
    },
    "scripts": {
        "build": "rm -rf dist/ && vite build --mode prod && vue-tsc --emitDeclarationOnly",
        "components": "rm -rf dist/ && rollup -c rollup.components.config.js",
        "lint:check": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
    },
    "devDependencies": {
        "@rollup/plugin-json": "^5.0.2",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.1",
        "@types/node": "^18.11.10",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vue/compiler-sfc": "^3.2.45",
        "@vue/server-renderer": "^3.2.45",
        "esbuild": "^0.15.17",
        "eslint": "^8.29.0",
        "i18next": "^22.0.6",
        "mitt": "^3.0.0",
        "pinia": "^2.0.27",
        "rollup": "^2.50.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-scss": "^4.0.0",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^4.9.3",
        "unplugin-auto-import": "^0.12.0",
        "unplugin-vue": "^3.1.3",
        "unplugin-vue-components": "^0.22.11",
        "unplugin-vue-macros": "^1.0.3",
        "vite": "^3.2.4",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.0.9"
    }
}
